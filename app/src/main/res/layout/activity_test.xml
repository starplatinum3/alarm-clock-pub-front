<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.MainActivity">
    <Button

        android:id="@+id/btn_to_show_log"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_to_show_log"


        app:layout_constraintStart_toStartOf="parent" />
    app:layout_constraintBottom_toBottomOf="parent"

    <Button
        android:id="@+id/btn_to_init_log_file"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_to_init_log_file"
        app:layout_constraintBottom_toTopOf="@id/btn_to_book_main"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_to_show_log" />

    <Button
        android:id="@+id/btn_to_book_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_to_book_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_to_init_log_file"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/btn_to_noti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_to_noti"
        app:layout_constraintBottom_toTopOf="@+id/btn_to_ring"
        app:layout_constraintStart_toStartOf="parent" />

    app:layout_constraintBottom_toBottomOf="parent"
    <Button
        android:id="@+id/btn_to_ring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_to_ring"
        app:layout_constraintTop_toBottomOf="@id/btn_to_noti"
        app:layout_constraintStart_toStartOf="parent" />
    <Button
        android:id="@+id/btn_to_log_table"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_to_log_table"
        />
    <Button
        android:id="@+id/btn_start_noti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="btn_start_noti"
        />
</LinearLayout>